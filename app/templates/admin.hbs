<h1>Administrator Console</h1>

<div class="row">
  <div class="col-md-7">
    <h2>Add New Story</h2>
    {{add-story save2="save3"}}
  </div>
  <div class="col-md-5">
    <h2>Edit Current Stories</h2>
    <div class="list-group">
      {{#each model as |headline|}}
        <button class="list-group-item">{{headline.headline}}</button>
      {{/each}}
    </div>

  </div>
</div>
